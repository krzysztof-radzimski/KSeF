<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <clear />
    <!--
      Źródło pakietów KSeF.Client z GitHub Packages (CIRFMF).
      Wymaga Personal Access Token (PAT) z uprawnieniem read:packages.

      Konfiguracja:
      dotnet nuget add source "https://nuget.pkg.github.com/CIRFMF/index.json" ^
        - -name github-cirf ^
        - -username token ^
        - -password TWOJ_PAT_TOKEN ^
        - -store-password-in-clear-text
    -->
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
    <add key="github-cirf" value="https://nuget.pkg.github.com/CIRFMF/index.json" />
  </packageSources>
</configuration>
